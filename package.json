{
  "name": "login-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "db:serve": "json-server --watch public/db.json --routes public/routes.json --port=8080",
    "dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:client": "npm run serve --prefix '../client/'",
    "start:server": "nodemon index.js --ignore '../client/'"
  },
  "dependencies": {
    "@mdi/font": "^7.3.67",
    "@popperjs/core": "^2.11.8",
    "axios": "^1.5.1",
    "bootstrap": "^5.3.2",
    "boxicons": "^2.1.4",
    "core-js": "^3.8.3",
    "dotenv": "^16.3.1",
    "element-plus": "^2.4.1",
    "json-server": "^0.17.4",
    "jsonwebtoken": "^9.0.2",
    "mdb-ui-kit": "^6.4.2",
    "node-modules": "^1.0.1",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "roboto-fontface": "*",
    "stripe": "^13.10.0",
    "vit": "^0.0.1",
    "vue": "^3.2.13",
    "vue-horizontal": "^1.0.2",
    "vue-i18n": "^9.5.0",
    "vue-router": "^4.0.3",
    "vue3-toastify": "^0.1.13",
    "vuesax": "^4.0.1-alpha.25",
    "vuetify": "^3.4.0",
    "vuex": "^4.0.0",
    "vuex-persistedstate": "^4.1.0",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@types/bootstrap": "^5.2.8",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "add": "^2.0.6",
    "concurrently": "^8.2.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "nodemon": "^3.0.1",
    "sass": "^1.69.5",
    "sass-loader": "^10.4.1",
    "stylus": "^0.60.0",
    "stylus-loader": "^7.1.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
